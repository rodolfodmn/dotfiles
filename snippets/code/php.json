	"Var_dump": {
		"prefix": "vd",
		"body": [
			"var_dump('$1');"
		],
		"description": "Manda um var_dump"
	},
	"Var_dump Exit": {
		"prefix": "vde",
		"body": [
			"var_dump('$1');",
			"exit"
		],
		"description": "Manda um var_dump com exit"
	},
	"Print_r pr": {
		"prefix": "pr",
		"body": [
			"echo '<pre>';",
			"print_r('$1');",
			"echo '</pre>';"
		],
		"description": "Manda um print_f com pre"
	},
	"Print_r pre Exit": {
		"prefix": "pre",
		"body": [
			"echo '<pre>';",
			"print_r('$1');",
			"exit;"
		],
		"description": "Manda um print_f com pre e um exit"
	},
	"Mage Log": {
		"prefix": "mg",
		"body": [
			"Mage::log($1, null, 'rdLog.log', true);"
		],
		"description": "Manda um Mage log"
	}


}